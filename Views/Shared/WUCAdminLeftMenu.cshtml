<script type="text/javascript">
    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
            $(this).toggleClass('active');
        });
    });

    window.onload = function () {
        highlightSelectedMenu();
    }
    function highlightSelectedMenu() {
        var currentURL = window.location.href;
        if (currentURL.includes("Admin/AddPlans")) {
            $("#liAddPlans").removeClass("Organization_Color");
            $("#liAddPlans").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/AdminMemberList")) {
            $("#liMemberList").removeClass("Organization_Color");
            $("#liMemberList").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/AdminMemberReport")) {
            $("#liMemberReport").removeClass("Organization_Color");
            $("#liMemberReport").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/AdminViewPaymentDetails")) {
            $("#liPaymentsdashboard").removeClass("Organization_Color");
            $("#liPaymentsdashboard").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/AdminPaymentsReport")) {
            $("#liPaymentsReport").removeClass("Organization_Color");
            $("#liPaymentsReport").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/AdminPaymentsTransactionReport")) {
            $("#liPaymentsTransactionReport").removeClass("Organization_Color");
            $("#liPaymentsTransactionReport").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/ViewPlans")) {
            $("#liViewPlans").removeClass("Organization_Color");
            $("#liViewPlans").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/WithdrawPlans")) {
            $("#liWithdrawPlans").removeClass("Organization_Color");
            $("#liWithdrawPlans").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/SubscribePlans")) {
            $("#liSubscribePlans").removeClass("Organization_Color");
            $("#liSubscribePlans").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/ViewOrganizations")) {
            $("#liViewOrganizations").removeClass("Organization_Color");
            $("#liViewOrganizations").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/ViewProviders")) {
            $("#liViewProviders").removeClass("Organization_Color");
            $("#liViewProviders").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/ViewMembers")) {
            $("#liViewMembers").removeClass("Organization_Color");
            $("#liViewMembers").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/ViewPayments")) {
            $("#liViewPayments").removeClass("Organization_Color");
            $("#liViewPayments").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/DiabledOrganizations")) {
            $("#liDiabledOrganizations").removeClass("Organization_Color");
            $("#liDiabledOrganizations").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/ViewUser")) {
            $("#liViewUser").removeClass("Organization_Color");
            $("#liViewUser").addClass("stripePanel");
        }
        else if (currentURL.includes("Admin/TermsAndConditions")) {
            $("#liTermsAndConditions").removeClass("Organization_Color");
            $("#liTermsAndConditions").addClass("stripePanel");
        }
    }
</script>