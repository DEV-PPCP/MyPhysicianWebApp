<script type="text/javascript">
    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
            $(this).toggleClass('active');
        });
    });

    window.onload = function () {
        highlightSelectedMenu();
    }
    function highlightSelectedMenu() {
        var currentURL = window.location.href;
        if (currentURL.includes("Organization/Addmember")) {
            $("#liAddMember").removeClass("Organization_Color");
            $("#liAddMember").addClass("stripePanel");
        }
        if (currentURL.includes("Organization/ViewMemberDetails")) {
            $("#liViewMember").removeClass("Organization_Color");
            $("#liViewMember").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/ViewPaymentDetails")) {
            $("#liViewPayments").removeClass("Organization_Color");
            $("#liViewPayments").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/ViewProviders")) {
            $("#liViewProviders").removeClass("Organization_Color");
            $("#liViewProviders").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/PendingPlanEnrollment")) {
            $("#liPendingEnrollmrntPlans").removeClass("Organization_Color");
            $("#liPendingEnrollmrntPlans").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/PendingPlanEnrollment")) {
            $("#liPaymentsReport").removeClass("Organization_Color");
            $("#liPaymentsReport").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/PendingPlanEnrollment")) {
            $("#liMembersReport").removeClass("Organization_Color");
            $("#liMembersReport").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/PendingPlanEnrollment")) {
            $("#liTransactionReport").removeClass("Organization_Color");
            $("#liTransactionReport").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/SubscribePlans")) {
            $("#liSubscribePlans").removeClass("Organization_Color");
            $("#liSubscribePlans").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/ViewUsers")) {
            $("#liViewUsers").removeClass("Organization_Color");
            $("#liViewUsers").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/OrganizationProfileDetails")) {
            $("#liOrganizationProfileDetailss").removeClass("Organization_Color");
            $("#liOrganizationProfileDetails").addClass("stripePanel");
        }
        else if (currentURL.includes("Organization/ChangePassword")) {
            $("#liChangePassword").removeClass("Organization_Color");
            $("#liChangePassword").addClass("stripePanel");
        }
    }
</script>
