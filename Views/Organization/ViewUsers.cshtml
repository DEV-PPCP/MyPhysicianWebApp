    function EditOptions(UserID, FirstName, LastName, CountryCode, MobileNumber, DOB, Gender, Email, CountryName, StateName, CityName, Zip, Salutation, CountryID, StateID, CityID, IsTwofactorAuthentication, TwoFactorType) {
       
        $("#fsLabOrderSearch").hide();
        $("#UserID").val(UserID);
        var x = UserID;
        $("#hdnUserID").val(x);
        $("#divMain").show();
        $("#divAddUser").hide();
        $("#divUserGrid").hide();
        //$("#divtwofactor").show();
        $("#divupdate").show();
        $("#FirstName").val(FirstName);
        $("#LastName").val(LastName);
        $("#txtCountry").val(CountryCode);
        $("#txtMobile").val(MobileNumber);
        var date = kendo.toString(kendo.parseDate(DOB), 'MM/dd/yyyy');
        $("#DOB").val(date);
        calulateage(date);
        $("#Gender").val(Gender);
        if (Gender == 2) {
            document.getElementById("rbtnGenderFemale").checked = true;
        }
        if (Gender == 1) {
            document.getElementById("rbtnGenderMale").checked = true;
        }

        $("#Email").val(Email);
        $("#CountryName").val(CountryName);


        $("#CountryID").val(CountryID);


        $("#StateName").val(StateName);


        $("#StateID").val(StateID);


        $("#CityName").val(CityName);


        $("#CityID").val(CityID);
        var combinedZip = Zip;
        var postalcode = combinedZip.split('-');
        $("#Zip").val(postalcode[0]);
        $("#ZipCode").val(postalcode[1]);
        $("#Salutation").val(Salutation);
        $('#chk2factor').prop('checked', true);
        $("#divEnableFactor").show();
        if (TwoFactorType == "1") {

            $('#rbtnEverytime').prop('checked', true);

        } else if (TwoFactorType == "2") {

            $('#rbtnOnlySysChange').prop('checked', true);
        }
        BindCountries('@ServiceUrl', CountryID);
        BindStates('@ServiceUrl', CountryID, StateID);
        BindCites('@ServiceUrl', StateID, CityID);
        BindSalutation(Salutation);

        $("#btnSubmitClick").click(function (e) {
            var EnrollPlanValidator = $("#divMain").kendoValidator().data("kendoValidator");
            if (EnrollPlanValidator.validate()) {
                btnSubmitOrganizationDetails();
            }
            else {
                return false;
            }
        });

    }
    function btnSubmitOrganizationDetails() {
        var jsModel = {};
        jsModel.UserID = $("#hdnUserID").val();
        jsModel.OrganizationName = $("#OrganizationName").val();
        jsModel.OrganizationID = $("#OrganizationId").val();
        jsModel.Type = 1;
        jsModel.FirstName = $("#FirstName").val();
        jsModel.LastName = $("#LastName").val();
        jsModel.DOB = $("#DOB").val();
        jsModel.Age = $("#Age").val();
        var rbtnMale = $('input[id=rbtnGenderMale]:checked').val();
        if (rbtnMale == 1)
            jsModel.Gender = rbtnMale;
        var rbtnFEM = $('input[id=rbtnGenderFemale]:checked').val();
        if (rbtnFEM == 2)
            jsModel.Gender = rbtnFEM;
        //jsModel.SalutationId = $("#SalutationId").val();
        jsModel.Salutation = $("#hdnSalutation").val();
        jsModel.Email = $("#Email").val();
        jsModel.CountryCode = $("#txtCountry").val();
        jsModel.MobileNumber = $("#txtMobile").val();
        jsModel.CountryName = $("#hdnCountryName").val();
        jsModel.CountryID = $("#hdnCountryID").val();


        jsModel.StateName = $("#hdnStateName").val();
        jsModel.StateID = $("#hdnStateID").val();
        jsModel.CityName = $("#hdnCityName").val();
        jsModel.CityID = $("#hdnCityID").val();
        if ($("#ZipCode").val() == "") {
            jsModel.Zip = $("#Zip").val();
        }
        else {
            jsModel.Zip = $("#Zip").val() + "-" + $("#ZipCode").val();
        }
        //veena
        if ($('#chk2factor').is(":checked") == true) {
            jsModel.IsTwofactorAuthentication = true;
            jsModel.PreferredIP = '@Session["SystemIPAddress"]';
            var rbtnEvery = $('input[id=rbtnEverytime]:checked').val();
            if (rbtnEvery == 1)
                jsModel.TwoFactorType = rbtnEvery;
            var rbtnSystem = $('input[id=rbtnOnlySysChange]:checked').val();
            if (rbtnSystem == 2)
                jsModel.TwoFactorType = rbtnSystem;
        }
        else {
            jsModel.IsTwofactorAuthentication = false;
            jsModel.TwoFactorType = "0";
            jsModel.PreferredIP = "";
        }
        //end veena
        AddUserDetails(jsModel,"@ServiceUrl");
    }

    function calulateage(date) {
        var birthDate = $("#DOB").val();
        $.ajax({
            url: "/Master/GetAge/",
            data: { birthDate: birthDate },
            dataType: 'json',
            contentType: 'application/json',
            success: function (result) {
                var rs = result.split(';');
                var year = rs[0];
                $("#Age").val(year);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            },
        });
    }

    function setCollapsePanel(dvSearchFilterHeader, dvSearchFilter) {
        $(dvSearchFilter).slideToggle('slow');
        $(dvSearchFilterHeader).find(".ArrowClose:first").remove();
        $("<div class='ArrowExpand'></div>").appendTo(dvSearchFilterHeader);
        isCollapse = 1;
    }
    var isCollapse = 0;
    function CollapsiblePanels() {
        $('#dvSearchFilter').slideToggle('slow');
        if (isCollapse == 0) {
            $("#dvSearchFilterHeader").find(".ArrowClose:first").remove();
            $("<div class='ArrowExpand'></div>").appendTo($("#dvSearchFilterHeader"));
            isCollapse = 1;
        }
        else {
            $("#dvSearchFilterHeader").find(".ArrowExpand:first").remove();
            $("<div class='ArrowClose'></div>").appendTo($("#dvSearchFilterHeader"));
            isCollapse = 0;
        }
        $("#arrow").hide();
    }
    function btnSearch() {
        var UserId=$("#UserId").val();
        BindOrganizationsUsersGrid(UserId, '@Session["OrganizationID"]', '@ServiceUrl');
    }
    function btnClear() {
        document.getElementById('UserName').value = '';
        //to clear grid
       BindOrganizationsUsersGrid(0, '@Session["OrganizationID"]', '@ServiceUrl');
    }
</script>
