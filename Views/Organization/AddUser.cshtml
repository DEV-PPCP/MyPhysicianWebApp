<script type="text/javascript">
    $(document).ready(function () {
        $("#CountryName").val("United States of America");
        $("#CountryId").val("1");
        BindOrganizations('@ServiceUrl');
        BindCountries("1", '@ServiceUrl');
        BindStates("1", "StateName", '@ServiceUrl', "1");
        BindCities("1", "CityName", '@ServiceUrl', "1");
    });
  
    function EditOptions(UserID, OrganizationID) {
        var webMethodName = "GetOrganizationUsers";
        var ParameterNames = new Array();
        var ParameterValues = new Array();
        ParameterNames[0] = "UserID";
        ParameterValues[0] = UserID;
        ParameterNames[1] = "OrganizationID";
        ParameterValues[1] = OrganizationID;
        var Url = "http://192.168.1.20/ppcpwebservice/OrganizationServices";       
        var jsonPostString = setJsonParameter(ParameterNames, ParameterValues, webMethodName);
        $.ajax({
            type: "POST",
            url: Url,
            data: jsonPostString,
            dataType: "text",
            contentType: "application/json",
            success: function (result) {
                var obj = jQuery.parseJSON(result);
                var PlansList = obj[0];
                $("#divViewUpdateUser").show();
                $("#divViewLoginCredentials").show();

                $("#divAddUser").hide();
                $("#divUserGrid").hide();
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            },
        });
    }
    function onDataBound() {
        var grid = this;
        $("[id='UsersGrid']").change(function (e) {
            var row = $(e.target).closest("tr");
            var dataItem = grid.dataItem(row);
            grid.select(row);
        });

    }
</script>
