    function btnSubmitOrganizationDetails() {
        var jsModel = {};
        jsModel.OrganizationName = $("#OrganizationName").val();
        jsModel.OrganizationID = $("#OrganizationId").val();
        jsModel.Type = 1;
        jsModel.FirstName = $("#FirstName").val();
        jsModel.LastName = $("#LastName").val();
        jsModel.DOB = $("#DOB").val();
        jsModel.Age = $("#Age").val();
        var rbtnMale = $('input[id=rbtnGenderMale]:checked').val();
        if (rbtnMale == 1)
            jsModel.Gender = rbtnMale;
        var rbtnFEM = $('input[id=rbtnGenderFemale]:checked').val();
        if (rbtnFEM == 2)
            jsModel.Gender = rbtnFEM;
        jsModel.SalutationId = $("#SalutationId").val();
        jsModel.Salutation = $("#UserSalutation").val();
        jsModel.Email = $("#Email").val();
        jsModel.CountryCode = $("#txtCountry").val();
        jsModel.MobileNumber = $("#txtMobile").val();
        jsModel.CountryName = $("#UserCountryName").val();
        jsModel.CountryId = $("#CountryId").val();
        jsModel.StateName = $("#UserStateName").val();
        jsModel.StateId = $("#StateId").val();
        jsModel.CityName = $("#UserCityName").val();
        jsModel.CityId = $("#CityId").val();
        if ($("#ZipCode").val() == "") {
            jsModel.Zip = $("#Zip").val();
        }
        else {
            jsModel.Zip = $("#Zip").val() + "-" + $("#ZipCode").val();
        }
        jsModel.UserName = $("#UserName").val();
        jsModel.Password = $("#Password").val();
        jsModel.ConfirmPassword = $("#ConfirmPassword").val();
        //veena
        if ($('#chk2factor').is(":checked") == true) {
            jsModel.IsTwofactorAuthentication = true;
            jsModel.PreferredIP = '@Session["SystemIPAddress"]';
            var rbtnEvery = $('input[id=rbtnEverytime]:checked').val();
            if (rbtnEvery == 1)
                jsModel.TwoFactorType = rbtnEvery;
            var rbtnSystem = $('input[id=rbtnOnlySysChange]:checked').val();
            if (rbtnSystem == 2)
                jsModel.TwoFactorType = rbtnSystem;
        }
        else {
            jsModel.IsTwofactorAuthentication = false;
            jsModel.TwoFactorType = "0";
            jsModel.PreferredIP = "";
        }
        //end veena
        AddUserDetails(jsModel,"@ServiceUrl");
    }


    function chkPasswordStrength(txtPassword, strenghtMsg, errorMsg) {    //To check Password strength (in Step1)
        var desc = new Array();
        desc[0] = "Very Weak";
        desc[1] = "Weak";
        desc[2] = "Average";
        desc[3] = "Strong";
        desc[4] = "Strong";
        desc[5] = "Strong";

        var score = 0;

        //if txtPassword bigger than 6 give 1 point
        if (txtPassword.length > 6) score++;

        //if txtPassword has both lower and uppercase characters give 1 point
        if ((txtPassword.match(/[a-z]/)) && (txtPassword.match(/[A-Z]/))) score++;

        //if txtPassword has at least one number give 1 point
        if (txtPassword.match(/\d+/)) score++;

        //if txtPassword has at least one special caracther give 1 point
        if (txtPassword.match(/.[!,#,$,%,^,&,*,?,_,~,-,(,)]/)) score++;

        //if txtPassword bigger than 12 give another 1 point
        if (txtPassword.length > 12) score++;

        strenghtMsg.innerHTML = desc[score];

        strenghtMsg.className = "strength" + score;
    }


