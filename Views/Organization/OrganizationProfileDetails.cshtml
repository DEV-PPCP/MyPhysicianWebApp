<script type="text/javascript">
    $(document).ready(function () {
        var ProfileDetails =@Html.Raw(Json.Encode(@Model));
        BindCountries('@ServiceUrl',ProfileDetails.OrgCountryID,ProfileDetails.CountryID);
        OrgBindStates('@ServiceUrl',ProfileDetails.OrgCountryID,ProfileDetails.OrgStateID);
        BindStates('@ServiceUrl',ProfileDetails.CountryID,ProfileDetails.StateID);
        OrgBindCites('@ServiceUrl',ProfileDetails.OrgStateID,ProfileDetails.OrgCityID);
        BindCites('@ServiceUrl',ProfileDetails.StateID,ProfileDetails.CityID);
        BindSalutation(ProfileDetails.Salutation);
        DateSelectionChanged(this);
        if (ProfileDetails.Gender == 1) {
            $("#rbtnGenderMale").prop("checked", true);
        }
        else {
            $("#rbtnGenderFemale").prop("checked", true);
        }

        if(ProfileDetails.IsTwofactorAuthentication==true){
            $('#chk2factor').prop('checked', true);
            $("#divEnableFactor").show();
            if(ProfileDetails.TwoFactorType == "1")
            {

                $('#rbtnEverytime').prop('checked', true);

            }else if(ProfileDetails.TwoFactorType == "2")
            {

                $('#rbtnOnlySysChange').prop('checked', true);
            }
        }
        else{
            $("#divEnableFactor").hide();
        }
        $("#OrganizationCountryName").val(ProfileDetails.OrgCountryName);
        $("#OrganizationStateName").val(ProfileDetails.OrgStateName);
        $("#OrganizationCityName").val(ProfileDetails.OrgCityName);
        $("#UserCountryName").val(ProfileDetails.CountryName);
        $("#UserStateName").val(ProfileDetails.StateName);
        $("#UserCityName").val(ProfileDetails.CityName);
        $("#UserSalutation").val(ProfileDetails.Salutation);
        $("#SalutationId").val(ProfileDetails.SalutationID);

        if (ProfileDetails.OrgZip.indexOf('_') == -1) {
            var arr =ProfileDetails.OrgZip.split('-');
            var zipcity=arr[0];
            $("#OrgZip").val(arr[0]);
            $("#OrgZipCode").val(arr[1]);
        }
        else {
            $("#OrgZip").val(ProfileDetails.OrgZip);
        }
        if (ProfileDetails.Zip.indexOf('_') == -1) {
            var arr = ProfileDetails.Zip.split('-');
            var zipcity=arr[0];
            $("#Zip").val(arr[0]);
            $("#ZipCode").val(arr[1]);
        }
        else {
            $("#Zip").val(ProfileDetails.Zip);
        }
    });

    function DateSelectionChanged(e) {
        var birthDate = $("#DOB").val();
        $.ajax({
            url: "/Master/GetAge/",
            data: { birthDate: birthDate },
            dataType: 'json',
            contentType: 'application/json',
            success: function (result) {
                var rs = result.split(';');
                var year = rs[0];
                $("#Age").val(year);
            }

        });
    }

    $("#chk2factor").click(function () {
        if ($('#chk2factor').is(":checked") == true) {
            $("#divEnableFactor").show();
        } else if ($('#chk2factor').is(":checked") == false) {
            $("#divEnableFactor").hide();
        }
    });
</script>
<script type="text/javascript">
    function btnSubmitOrganizationDetails() {
        debugger;
        var EnrollPlanValidator = $("#divMain").kendoValidator().data("kendoValidator");
        if (EnrollPlanValidator.validate()) {
            var ProfileDetails =@Html.Raw(Json.Encode(@Model));
            var jsModel = {};
            jsModel.OrganizationName =$("#OrganizationName").val(); ;
            jsModel.UserID = ProfileDetails.UserID;
            jsModel.OrganizationID = ProfileDetails.OrganizationID;
            jsModel.TaxID = $("#TaxID").val();
            jsModel.OrgEmail = $("#OrgEmail").val();
            jsModel.OrgCountryCode = $("#OrgCountryCode").val();
            jsModel.OrgMobileNumber = $("#OrgMobileNumber").val();
            jsModel.OrgCountryId = $("#OrgCountryId").val();
            jsModel.OrgCountryName = $("#OrganizationCountryName").val();
            jsModel.OrgStateId = $("#OrgStateId").val();
            jsModel.OrgStateName = $("#OrganizationStateName").val();
            jsModel.OrgCityId = $("#OrgCityId").val();
            jsModel.OrgCityName = $("#OrganizationCityName").val();
            jsModel.OrgZip = $("#OrgZip").val();
            jsModel.OrgZipCode = $("#OrgZipCode").val();
            jsModel.Address = $("#Address").val();
            jsModel.Type = 1;
            jsModel.FirstName = $("#FirstName").val();
            jsModel.LastName = $("#LastName").val();
            jsModel.DOB = $("#DOB").val();
            jsModel.Age = $("#Age").val();
            var rbtnMale = $('input[id=rbtnGenderMale]:checked').val();
            if (rbtnMale == 1)
                jsModel.Gender = rbtnMale;
            var rbtnFEM = $('input[id=rbtnGenderFemale]:checked').val();
            if (rbtnFEM == 2)
                jsModel.Gender = rbtnFEM;
            jsModel.SalutationId = $("#SalutationId").val();
            jsModel.Salutation = $("#UserSalutation").val();
            jsModel.Email = $("#Email").val();
            jsModel.CountryCode = $("#txtCountry").val();
            jsModel.MobileNumber = $("#txtMobile").val();
            jsModel.CountryName = $("#UserCountryName").val();
            jsModel.CountryId = $("#CountryId").val();
            jsModel.StateName = $("#UserStateName").val();
            jsModel.StateId = $("#StateId").val();
            jsModel.CityName = $("#UserCityName").val();
            jsModel.CityId = $("#CityId").val();
            jsModel.Zip = $("#Zip").val();
            jsModel.ZipCode = $("#ZipCode").val();
            debugger;
            jsModel.SSN=$("#SSN").val();
            jsModel.UserName = $("#UserName").val();
            jsModel.Password = $("#Password").val();
            jsModel.ConfirmPassword = $("#ConfirmPassword").val();
            if ($('#chk2factor').is(":checked") == true) {
                jsModel.IsTwofactorAuthentication = true;
                jsModel.PreferredIP = '@Session["SystemIPAddress"]';
                var rbtnEvery = $('input[id=rbtnEverytime]:checked').val();
                if (rbtnEvery == 1)
                    jsModel.TwoFactorType = rbtnEvery;
                var rbtnSystem = $('input[id=rbtnOnlySysChange]:checked').val();
                if (rbtnSystem == 2)
                    jsModel.TwoFactorType = rbtnSystem;
            }
            else {
                jsModel.IsTwofactorAuthentication = false;
                jsModel.TwoFactorType = "0";
                jsModel.PreferredIP = "";
            }

            UpdateOrganizationProfile(jsModel,'@ServiceUrl');
        }
    }

    function btnCloseOrganizationDetails() {
        var getUrl = '@Url.Action("OrganizationLogin", "Organization")';
        window.location = getUrl;
    }
</script>
