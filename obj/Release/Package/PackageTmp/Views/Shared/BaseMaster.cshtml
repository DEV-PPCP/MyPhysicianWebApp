@*<script src="~/Scripts/jquery-1.12.0.min.js" type="text/javascript"></script>*@
<script src="~/Scripts/bootstrap-leftmenu.js" type="text/javascript"></script>

<script type="text/javascript">
    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
            $(this).toggleClass('active');
        });
    });

    window.onload = function () {
        highlightSelectedMenu();
    }
    function highlightSelectedMenu() {
        var currentURL = window.location.href;
        if (currentURL.includes("Member/FamilyMember")) {
            $("#liFamilyMember").removeClass("Organization_Color");
            $("#liFamilyMember").addClass("stripePanel");
        }
        else if (currentURL.includes("Member/PlanEnroll")) {
            $("#liPlanEnroll").removeClass("Organization_Color");
            $("#liPlanEnroll").addClass("stripePanel");
        }
        else if (currentURL.includes("Member/PlanDetails")) {
            $("#liPlanDetails").removeClass("Organization_Color");
            $("#liPlanDetails").addClass("stripePanel");
        }
        else if (currentURL.includes("Member/PaymentDetails")) {
            $("#liPaymentDetails").removeClass("Organization_Color");
            $("#liPaymentDetails").addClass("stripePanel");
        }
        else if (currentURL.includes("Member/ChangePassword")) {
            $("#liChangePassword").removeClass("Organization_Color");
            $("#liChangePassword").addClass("stripePanel");
        }        
    }
</script>
