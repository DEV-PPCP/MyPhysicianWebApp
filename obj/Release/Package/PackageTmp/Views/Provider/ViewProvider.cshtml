<script>
    $(document).ready(function () {
        debugger;
        var ProfileDetails =@Html.Raw(Json.Encode(@Model));
        // BindOrganizations("@ServiceUrl",ProfileDetails.OrganizationID);
        BindCountries('@ServiceUrl',ProfileDetails.CountryID);
        BindStates('@ServiceUrl',ProfileDetails.CountryID,ProfileDetails.StateID);
        BindCities('@ServiceUrl',ProfileDetails.StateID,ProfileDetails.CityID);
        BindSalutation(ProfileDetails.Salutation);
        debugger;
     
        DateSelectionChanged(this);


        if (ProfileDetails.Gender == 1) {
            $("#rbtnGenderMale").prop("checked", true);
        }
        else {
            $("#rbtnGenderFemale").prop("checked", true);
        }


        debugger;
        if(ProfileDetails.IsTwofactorAuthentication==true){
            $('#chk2factor').prop('checked', true);
            $("#divEnableFactor").show();
            if(ProfileDetails.TwoFactorType == "1")
            {

                $('#rbtnEverytime').prop('checked', true);

            }else if(ProfileDetails.TwoFactorType == "2")
            {

                $('#rbtnOnlySysChange').prop('checked', true);
            }
        }
        else{
            $("#divEnableFactor").hide();
        }
        debugger;

        $("#TempCountryName").val(ProfileDetails.CountryName);
        $("#TempStateName").val(ProfileDetails.StateName);
        $("#TempCityName").val(ProfileDetails.CityName);
        $("#UserSalutation").val(ProfileDetails.Salutation);
        $("#SalutationId").val(ProfileDetails.SalutationID);


        if (ProfileDetails.Zip.indexOf('_') == -1) {
            var arr = ProfileDetails.Zip.split('-');
            var zipcity=arr[0];
            $("#Zip").val(arr[0]);
            $("#ZipCode").val(arr[1]);
        }
        else {
            $("#Zip").val(ProfileDetails.Zip);
        }
        GetSpecilization("@ServiceUrl",ProfileDetails.SpecializationID);
        debugger;
        BindDegree(ProfileDetails.Degree);
    });
    function BindDegree(strDegree) {
        debugger;
        $.ajax({
            type: 'POST',
            cache: false,
            url: '/Master/BindDegree',
            //data: ,
            success: function (SalutationList, textStatus, jqXHR) {
                for (var r in SalutationList) {
                    $('#Degree').data('kendoDropDownList').dataSource.insert(r, { Text: SalutationList[r].Text, Value: SalutationList[r].ID });
                }          
                var DegreeName = strDegree;
                if (DegreeName != "" && DegreeName != null) {
                    var dropdownlist = $("#Degree").data("kendoDropDownList");
                    dropdownlist.select(function (dataItem) {
                        return dataItem.Text === DegreeName;
                    });
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            },
        });
    }

    function DateSelectionChanged(e) {
        debugger;
        var birthDate = $("#DOB").val();
        $.ajax({
            url: "/Master/GetAge/",
            data: { birthDate: birthDate },
            dataType: 'json',
            contentType: 'application/json',
            success: function (result) {
                var rs = result.split(';');
                var year = rs[0];
                $("#Age").val(year);
            }

        });
    }

    $("#chk2factor").click(function () {
        debugger;
        if ($('#chk2factor').is(":checked") == true) {
            $("#divEnableFactor").show();
        } else if ($('#chk2factor').is(":checked") == false) {
            $("#divEnableFactor").hide();
        }
    });
    function CreateJosn(array) {
        debugger;
        jsonObj = [];
        var tempPayment = array.toString().split(',');
        for (i = 0; i < tempPayment.length; i++) {
            var temp = tempPayment[i].split(';');
            item = {};
            item["SpecilizationID"] = temp[0];
            item["SpecializationName"] = temp[1];

            jsonObj.push(item);
        }
        return jsonObj;
    }
    function btnSaveProviderDetails() {
        debugger;
        var jsModel = {};
        var ProfileDetails =@Html.Raw(Json.Encode(@Model));


        var EnrollPlanValidator = $("#divMainAddDoctors").kendoValidator().data("kendoValidator");
        if (EnrollPlanValidator.validate()) {
            SpecializationValidation();

            jsModel.ProviderID = ProfileDetails.ProviderID;

            jsModel.FirstName = $("#FirstName").val();
            jsModel.LastName = $("#LastName").val();
            jsModel.Salutation = $("#Salutation").val();
            jsModel.SalutationID = $("#SalutationId").val();
            jsModel.DOB = $("#DOB").val();
            var rbtnMale = $('input[id=rbtnGenderMale]:checked').val();
            if (rbtnMale == 1)
                jsModel.Gender = rbtnMale;
            var rbtnFEM = $('input[id=rbtnGenderFemale]:checked').val();
            if (rbtnFEM == 2)
                jsModel.Gender = rbtnFEM;
            jsModel.CountryCode = $("#CountryCode").val();
            jsModel.MobileNumber = $("#MobileNumber").val();
            jsModel.Email = $("#Email").val();
            jsModel.CityID = $("#CityId").val();
            jsModel.CityName = $("#TempCityName").val();
            jsModel.StateName = $("#TempStateName").val();
            jsModel.StateID = $("#StateID").val();
            jsModel.CountryID = $("#CountryId").val();
            jsModel.CountryName = $("#TempCountryName").val();
            jsModel.NPI = $("#NPI").val();
            if ($("#ZipCode").val() == "")
            {
                jsModel.Zip = $("#Zip").val();
            }
            else {
                jsModel.Zip = $("#Zip").val()+ "-"+ $("#ZipCode").val();
            }

            jsModel.UserName = $("#UserName").val();
            jsModel.Password = $("#Password").val();
            jsModel.ConfirmPassword = $("#ConfirmPassword").val();
            jsModel.Fax=$("#Fax").val();
            jsModel.Address=$("#Address").val();
            jsModel.Degree=$("#Degree").val();

            var SpecilizationID = "", SpecilizationName = "";
            var checkboxes = document.querySelectorAll('input[name="SPList"]:checked');
            for (var i = 0; i < checkboxes.length; i++) {

                var tempSplit = checkboxes[i].value.split(";");
                SpecilizationID = SpecilizationID + "" + tempSplit[0] + ",";
                SpecilizationName = SpecilizationName + "" + tempSplit[1] + ",";

            }

            var array = [];
            var checkboxes = document.querySelectorAll('input[name="SPList"]:checked')
            for (var i = 0; i < checkboxes.length; i++) {
                array.push(checkboxes[i].value);
            }
            jsModel.SpecializationID = SpecilizationID;
            jsModel.SpecializationName = SpecilizationName.substring(0, SpecilizationName.length - 1);
            var obj = CreateJosn(array);
            jsModel.Specialization = JSON.stringify(obj);

            if ($('#chk2factor').is(":checked") == true) {
                debugger;
                jsModel.IsTwofactorAuthentication = true;
                jsModel.PreferredIP = '@Session["SystemIPAddress"]';
                var rbtnEvery = $('input[id=rbtnEverytime]:checked').val();
                if (rbtnEvery == 1)
                    jsModel.TwoFactorType = rbtnEvery;
                var rbtnSystem = $('input[id=rbtnOnlySysChange]:checked').val();
                if (rbtnSystem == 2)
                    jsModel.TwoFactorType = rbtnSystem;
            }
            else {
                jsModel.IsTwofactorAuthentication = false;
                jsModel.TwoFactorType = "0";
                jsModel.PreferredIP = "";
            }

            SaveDoctorDetails(jsModel, "@ServiceUrl");
        }
        else{


            $("#divSpecializationValidator").show();
        }
    }

    function SpecializationValidation(){


        var checkboxes = document.querySelectorAll('input[name="SPList"]:checked');
        if(checkboxes==""){
            document.getElementById("divSpecializationValidator").style.display = "block";
        }

        else {
            document.getElementById("divSpecializationValidator").style.display = "none";
        }
    }
    function ErrorOk(){
        $("#divErrMessagePopup").hide();
    }
</script>
