<script type="text/javascript">
    function btnSubmitOrganizationDetails() {
        debugger;
        var EnrollPlanValidator = $("#divMainAddDoctors").kendoValidator().data("kendoValidator");
        if (EnrollPlanValidator.validate()) {
            debugger;
            var ProfileDetails =@Html.Raw(Json.Encode(@Model));
            var jsModel = {};
            jsModel.OrganizationName =$("#OrganizationName").val(); ;
            jsModel.ProviderID = ProfileDetails.ProviderID;
            jsModel.OrganizationID = ProfileDetails.OrganizationID;
            jsModel.TaxID = $("#TaxID").val();
            jsModel.OrgEmail = $("#OrgEmail").val();
            jsModel.OrgCountryCode = $("#OrgCountryCode").val();
            jsModel.OrgMobileNumber = $("#OrgMobileNumber").val();
            jsModel.OrgCountryId = $("#OrgCountryId").val();
            jsModel.OrgCountryName = $("#OrganizationCountryName").val();
            jsModel.OrgStateId = $("#OrgStateId").val();
            jsModel.OrgStateName = $("#OrganizationStateName").val();
            jsModel.OrgCityId = $("#OrgCityId").val();
            jsModel.OrgCityName = $("#OrganizationCityName").val();
            jsModel.OrgZip = $("#OrgZip").val();
            jsModel.OrgZipCode = $("#OrgZipCode").val();           
            jsModel.Fax=$("#Fax").val();
            jsModel.Address=$("#Address").val();
            jsModel.Degree=$("#Degree").val();
            jsModel.Type = 1;
            jsModel.FirstName = $("#FirstName").val();
            jsModel.LastName = $("#LastName").val();
            jsModel.DOB = $("#DOB").val();
            jsModel.Age = $("#Age").val();
            var rbtnMale = $('input[id=rbtnGenderMale]:checked').val();
            if (rbtnMale == 1)
                jsModel.Gender = rbtnMale;
            var rbtnFEM = $('input[id=rbtnGenderFemale]:checked').val();
            if (rbtnFEM == 2)
                jsModel.Gender = rbtnFEM;
            jsModel.SalutationId = $("#SalutationId").val();
            jsModel.Salutation = $("#UserSalutation").val();
            jsModel.Email = $("#Email").val();
            jsModel.CountryCode = $("#txtCountry").val();
            jsModel.MobileNumber = $("#txtMobile").val();
            jsModel.CountryName = $("#TempCountryName").val();
            jsModel.CountryId = $("#CountryId").val();
            jsModel.StateName = $("#TempStateName").val();
            jsModel.StateId = $("#StateID").val();
            jsModel.CityName = $("#TempCityName").val();
            jsModel.CityId = $("#CityId").val();
            jsModel.Zip = $("#Zip").val();
            jsModel.ZipCode = $("#ZipCode").val();

            jsModel.UserName = $("#UserName").val();
            jsModel.Password = $("#Password").val();
            jsModel.ConfirmPassword = $("#ConfirmPassword").val();
            debugger;
            if ($('#chk2factor').is(":checked") == true) {
                jsModel.IsTwofactorAuthentication = true;
                jsModel.PreferredIP = '@Session["SystemIPAddress"]';
                var rbtnEvery = $('input[id=rbtnEverytime]:checked').val();
                if (rbtnEvery == 1)
                    jsModel.TwoFactorType = rbtnEvery;
                var rbtnSystem = $('input[id=rbtnOnlySysChange]:checked').val();
                if (rbtnSystem == 2)
                    jsModel.TwoFactorType = rbtnSystem;
            }
            else {
                jsModel.IsTwofactorAuthentication = false;
                jsModel.TwoFactorType = "0";
                jsModel.PreferredIP = "";
            }

            SaveDoctorDetails(jsModel,'@ServiceUrl');
        }
    }

    function btnCloseOrganizationDetails() {
        var getUrl = '@Url.Action("ProviderLogin", "Provider")';
        window.location = getUrl;
    }
</script>
