                                                                @Html.Label("Username", new { style = "font-weight:normal;", @class = "" })<span style="color: Red;">*</span><br />
                                                                @Html.TextBoxFor(model => model.UserName, null, new { style = "width:100%;", maxlength = 25, onchange = "ValidateUserName(this.value);" })
                                                                @*@Html.ValidationMessageFor(model => model.UserName, "", new { @style = "color:red" })*@
                                                                <span id="spnUserName" style="color:red"></span><span id="spnUserNameValidator" style="color:red;display:none;" >This Information is Requried</span>
                                                            </div>
                                                            <div class="col-xs-12 col-md-3 col-sm-6 col-lg-3">
                                                                @Html.Label("Password", new { style = "font-weight:normal;", @class = "" })<span style="color: Red;">*</span><br />
                                                                @Html.TextBoxFor(model => model.Password, null, new { style = "width:100%;", maxlength = 20, onchange = "chkPasswordStrength(this.value);", type = "password", onkeyup = "chkPasswordStrength(this.value,document.getElementById('strength'),document.getElementById('error'))", onpaste = "return false;", oncopy = "return false", oncut = "return false" })
                                                                @*@Html.ValidationMessageFor(model => model.Password, "", new { @style = "color:red" })*@
                                                                <span id="strength" class="strength5"></span><span id="spnUserpasswordValidator" style="color:red;display:none;">This Information is Requried</span>
                                                            </div>
                                                            <div class="col-xs-12 col-md-3 col-sm-6 col-lg-3">
                                                                @Html.Label("Confirm Password", new { style = "font-weight:normal;", @class = "" })<span style="color: Red;">*</span><br />
                                                                @Html.TextBoxFor(model => model.ConfirmPassword, null, new { style = "width:100%;", maxlength = 20, type = "password", onchange = "validatePassword(this.value)", onpaste = "return false;", oncopy = "return false", oncut = "return false" })
                                                                @*@Html.ValidationMessageFor(model => model.ConfirmPassword, "", new { @style = "color:red" })*@
                                                                <span id="PasswordErrorMsg" style="color:red"></span> <span id="spnPUserpasswordValidator" style="color:red;display:none;">This Information is Requried</span>
                                 </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-xs-12 col-md-12 col-sm-12 col-lg-12">
                                                    @Html.CheckBox("chk2factor", false)&nbsp;<b>Enable 2-factor authentication</b>
                                                </div>
                                            </div>
                                            <div class="row" id="divEnableFactor" style="display:none;">
                                                <div class="col-xs-10 col-md-3 col-sm-6 col-lg-10">
                                                    <label style="font-weight: normal;">
                                                        @(Html.RadioButton("ChkList", 1, new
                                                                {
                                                                    id = "rbtnEverytime",
                                                                }))
                                                        Everytime
                                                    </label>&nbsp;&nbsp;&nbsp;&nbsp;
                                                    <label style="font-weight: normal;">
                                                        @(Html.RadioButton("ChkList", 2, new
                                                                {
                                                                    id = "rbtnOnlySysChange"
                                                                }))
                                                        Only when system change
                                                    </label>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-xs-12 col-md-12 col-sm-12" align="center">
                                        <button class="button_blue" type="submit" id="btnSubmit" title="Submit" style="width: 100px;" value="Submit">
                                            Submit
                                        </button>
                                        <button class="button_blue" type="button" id="btnCancel" title="Cancel" value="Cancel" style="width: 100px;" onclick="@("window.location.href='" + @Url.Action("MemberLogin", "Member") + "'");">
                                            Cancel
                                        </button>
                                    </div>
                                    <br />
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </fieldset>
</div>
