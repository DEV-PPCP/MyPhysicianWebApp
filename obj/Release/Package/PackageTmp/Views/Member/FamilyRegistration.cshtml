<script type="text/javascript">
    $(document).ready(function () {
        var countryID = 1;
        $('#CountryID').val("1");
        $("#CountryName").val("United States of America");
        var stateID = 1;
        $('#StateID').val(stateID);
        GetIpAddress();
        BindCountries('@ServiceUrl');
        BindStates(countryID,'@ServiceUrl');
        BindCities(stateID,'@ServiceUrl');
        GetRelationShip("@ServiceUrl");

        $("#chkMemberCredentials").click(function () {

            if ($('#chkMemberCredentials').is(":checked") == true) {
                $("#MemberCredentials").show();
            } else if ($('#chkMemberCredentials').is(":checked") == false) {
                $("#MemberCredentials").hide();
            }
        });
        $("#chk2factor").click(function () {
            if ($('#chk2factor').is(":checked") == true) {
                $("#divEnableFactor").show();
            } else if ($('#chk2factor').is(":checked") == false) {
                $("#divEnableFactor").hide();
            }
        });
        function MembeDetail()
        {
            var MemberRegistrationDetails =@Html.Raw(Json.Encode(@Model));
            FamilyMembeDetails(MemberRegistrationDetails,"@Session["SystemIPAddress"]",'@ServiceUrl','@Session["MemberParentID"]');
        }

        $("#btnSubmit").click(function (e) {
            var MemberDetailsvalidate = $("#fsMain").kendoValidator().data("kendoValidator");
            
            if ($('#chkMemberCredentials').is(":checked") == true) {
                MemberDetailsvalidate.validate();
                if( $("#UserName").val()=="" ||$("#Password").val()==""){
                  
                    $("#spnUserNameValidator").show();
                    $("#spnUserpasswordValidator").show();
                    $("#spnPUserpasswordValidator").show();
                   
                    return false;
                } 
                else{
                
                    $("#spnUserNameValidator").hide();
                    $("#spnUserpasswordValidator").hide();
                    $("#spnPUserpasswordValidator").hide();
                    MembeDetail();
                }
               }
            
            else{
               
                if(MemberDetailsvalidate.validate()){
                    MembeDetail();
                }
                else{
                    return false;
                }
            }
        });
    });
    function DateSelectionChanged(e) {
        var birthDate = $("#DOB").val();
        $.ajax({
            url: "/Master/GetAge/",
            data: { birthDate: birthDate },
            dataType: 'json',
            contentType: 'application/json',
            success: function (result) {
                var rs = result.split(';');
                var year = rs[0];
                $("#Age").val(year);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            },
        });
    }
   
    ///To check Password strength in MemberRegistration  : Ragini on 14-08-2019
    function chkPasswordStrength(txtPassword, strenghtMsg, errorMsg) {
        if(txtPassword!=""){
            $("#spnUserpasswordValidator").hide();
            $("#spnPUserpasswordValidator").hide();
        }
        var desc = new Array();

        desc[0] = "Very Weak";
        desc[1] = "Weak";
        desc[2] = "Average";
        desc[3] = "Strong";
        desc[4] = "Strong";
        desc[5] = "Strong";

        var score = 0;

        //if txtPassword bigger than 6 give 1 point
        if (txtPassword.length > 6) score++;

        //if txtPassword has both lower and uppercase characters give 1 point
        if ((txtPassword.match(/[a-z]/)) && (txtPassword.match(/[A-Z]/))) score++;

        //if txtPassword has at least one number give 1 point
        if (txtPassword.match(/\d+/)) score++;

        //if txtPassword has at least one special caracther give 1 point
        if (txtPassword.match(/.[!,#,$,%,^,&,*,?,_,~,-,(,)]/)) score++;

        //if txtPassword bigger than 12 give another 1 point
        if (txtPassword.length > 12) score++;

        strenghtMsg.innerHTML = desc[score];

        strenghtMsg.className = "strength" + score;
    }
    //Compair passwords in FamilyMembers
    function validatePassword() {
        //To Compare Password and ConfirmPassword (in Step1)
        $("#spnUserpasswordValidator").hide();
        if (($("#Password").val()) == ($("#ConfirmPassword").val())) {
            document.getElementById("PasswordErrorMsg").innerHTML = "";
        }
        else {
            document.getElementById("PasswordErrorMsg").innerHTML = "Password and Confirm Password should be Match.";
        }
    }
   
    function ValidateUserName(UserName) {
        $("#spnUserNameValidator").hide();
        ValidateUserNames(UserName,'@ServiceUrl');
    }
</script>
